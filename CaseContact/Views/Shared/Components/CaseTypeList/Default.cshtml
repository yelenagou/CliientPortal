@model IEnumerable<CaseContact.Models.CaseType>

<h2>View</h2>


<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.CaseTypeDescription)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td id="hiddenField" style="display:none">
                @Html.DisplayTextFor(modelItem => item.Id)
            </td>
            <td id="caseTypeDescription">
                @Html.DisplayFor(modelItem => item.CaseTypeDescription)
            </td>
          
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id" class="btn btn-success">Edit</a> 
                <a asp-action="Delete" asp-route-id="@item.Id" class="btn btn-warning">Delete</a>

            </td>
        </tr>
}
    </tbody>
</table>

